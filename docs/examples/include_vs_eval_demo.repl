// Include vs Eval demonstration
// Save as: examples/include_vs_eval_demo.repl  
// Run with: ./cpprepl -s -v -r examples/include_vs_eval_demo.repl

// ✅ SAFE: Standard library headers
#include <vector>
#include <iostream>
#include <algorithm>

// ✅ SAFE: Custom headers (declarations only)
// #include "myheader.h"  // Would work if file exists

// ✅ SAFE: Use #eval for .cpp files with globals
// This prevents double-free issues with global destructors
// #eval math_functions.cpp

// Create some test data
std::vector<int> data = {3, 1, 4, 1, 5, 9, 2, 6, 5};

// Sort the data
std::sort(data.begin(), data.end());

// Print sorted result
void print_data() {
    std::cout << "Sorted data: ";
    for (auto x : data) {
        std::cout << x << " ";
    }
    std::cout << std::endl;
}

print_data();

